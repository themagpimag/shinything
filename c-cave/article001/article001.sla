            <para ALIGN="0" LINESP="12"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="  CC = gcc"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="  TARGET = hist"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="  OBJECTS = $(patsubst %.c,%.o, $(wildcard *.c))"/>
            <para ALIGN="0" LINESP="12"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="  $(TARGET): $(OBJECTS)"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="        @echo &quot;** Linking Executable&quot;"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="        $(CC) $(OBJECTS) -o $(TARGET)"/>
            <para ALIGN="0" LINESP="12"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="  clean:"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="        @rm -f *.o *~"/>
            <para ALIGN="0" LINESP="12"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="  veryclean: clean"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="        @rm -f $(TARGET)"/>
            <para ALIGN="0" LINESP="12"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="  %.o: %.c"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="        @echo &quot;** Compiling C Source&quot; "/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT CH="        $(CC) -c $&lt;"/>
