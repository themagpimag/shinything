            <para ALIGN="0" LINESP="12"/>
            <para PARENT="4pt padding"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="10" FCOLOR="Black" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" CH="  xStdDev = 0.;"/>
            <para ALIGN="0" LINESP="12"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="10" FCOLOR="Black" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" CH="  for(i=1;i&lt;=nBins;i++) { /* Skip underflow and overflow */"/>
            <para ALIGN="0" LINESP="12"/>
