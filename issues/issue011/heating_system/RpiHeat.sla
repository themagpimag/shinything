            <ITEXT CH="//Initialize I2C bus"/>
            <para/>
            <ITEXT FONT="Courier10 BT Roman" FONTSIZE="10" CH="int fd;"/>
            <para/>
            <ITEXT FONT="Courier10 BT Roman" FONTSIZE="10" CH="char *fileName = &quot;/dev/i2c-1&quot;;"/>
            <para/>
            <ITEXT FONT="Courier10 BT Roman" FONTSIZE="10" CH="int  address = 0x31;     "/>
            <para/>
            <ITEXT CH="unsigned char buf[10]; "/>
            <para/>
            <ITEXT CH="fd = open(fileName, O_RDWR);"/>
            <para/>
            <ITEXT CH="ioctl(fd, I2C_SLAVE, address);"/>
            <para/>
            <ITEXT CH="//Select ADC channel 7"/>
            <para/>
            <ITEXT CH="buf[0] = 1;    "/>
            <para/>
            <ITEXT CH="buf[1] = 7;"/>
            <para/>
            <ITEXT CH="write(fd, buf, 2);"/>
            <para/>
            <ITEXT CH="//Start conversion"/>
            <para/>
            <ITEXT CH="buf[0] = 2;"/>
            <para/>
            <ITEXT CH="write(fd, buf, 1);"/>
            <para/>
            <ITEXT CH="//Fetch result of conversion"/>
            <para/>
            <ITEXT CH="buf[0] = 4;"/>
            <para/>
            <ITEXT CH="write(fd, buf, 1);"/>
            <para/>
            <ITEXT CH="read(fd, buf, 2);"/>
            <para/>
            <ITEXT CH="unsigned short packedword;"/>
            <para/>
            <ITEXT CH="packedword = (buf[0] &lt;&lt;8) | buf[1]; "/>
            <trail/>
