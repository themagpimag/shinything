            <ITEXT CH="//Initialize I2C bus"/>
            <para PARENT="Code"/>
            <ITEXT CH="int fd;"/>
            <para PARENT="Code"/>
            <ITEXT CH="char *fileName = &quot;/dev/i2c-1&quot;;"/>
            <para PARENT="Code"/>
            <ITEXT CH="//Note: When using a Rev 1 Pi, use &quot;/dev/i2c-0&quot; "/>
            <para PARENT="Code"/>
            <ITEXT CH="int  address = 0x31;     "/>
            <para PARENT="Code"/>
            <ITEXT CH="unsigned char buf[10]; "/>
            <para PARENT="Code"/>
            <ITEXT CH="fd = open(fileName, O_RDWR);"/>
            <para PARENT="Code"/>
            <ITEXT CH="ioctl(fd, I2C_SLAVE, address);"/>
